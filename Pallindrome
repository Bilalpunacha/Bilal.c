#include <stdio.h>

int main()

{

    int rem=0,rev=0,n,a;

    printf("enter the value of n");

    scanf("%d",&n);

    

    a=n;

    while(n!=0)

    {

        rem=n%10;

        rev=rev*10+rem;

        n=n/10;

    }

    if(a==rev)

        printf("%d is pallindrome",a);

    else

        printf("%d is not a pallindrome",a);

}
